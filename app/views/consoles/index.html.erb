<% if user_signed_in? %>
<div class="container">
  <h1 class="title">Game console rental</h1>
  <div class="banner"></div>
  <br>
    <div style="text-align:center">
      <p class="description">
        Rent a <strong>new generation console</strong> for a day, a week, or a month. And get the latest games !
      <br>
        Do not hesitate to contact us for advice at 01 74 50 03 51.
      </p>
    </div>
    <h2 class="title"> Our available game consoles </h2>

    <div class="cards">
    <% @consoles.each do |console| %>
      <div class="card">
        <% if console.name == "PS2" %>
          <img src="https://wallpaperaccess.com/full/1389554.jpg" alt="image de PS2">
        <% elsif console.name == "Nintendo DS Lite" %>
          <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/0/07/Nintendo-ds-lite-black.jpg/1280px-Nintendo-ds-lite-black.jpg" alt="Image de Nintendo DS Lite">
        <% elsif console.name == "XBOX" %>
          <img src="https://images.unsplash.com/photo-1621259182978-fbf93132d53d?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8Mnx8eGJveHxlbnwwfHwwfHw%3D&w=1000&q=80" alt="Image de XBOX">
        <% elsif console.name == "PS1" %>
          <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/9/95/PSX-Console-wController.png/1200px-PSX-Console-wController.png" alt="Image de PS1">
        <% elsif console.name == "PS3" %>
          <img src="https://img.redbull.com/images/c_limit,w_1500,h_1000,f_auto,q_auto/redbullcom/2014/08/21/1331673228513_2/playstation-console-jeu-video" alt="Image de PS3">
        <% elsif console.name == "Nintendo Switch" %>
          <img src="https://wallpapercave.com/wp/wp2241800.png" alt="Image de Nintendo Switch">
        <% elsif console.name == "Gameboy" %>
          <img src="https://thumbs.dreamstime.com/b/nintendo-gameboy-et-jeux-classiques-114898364.jpg" alt="Image de Gameboy">
        <% elsif console.name == "Game cube" %>
          <img src="https://images7.alphacoders.com/380/380296.jpg" alt="Image de Game Cube">
        <% elsif console.name == "WII" %>
          <img src="https://wallpapercave.com/wp/wp7323087.jpg" alt="Image de WII">
        <% elsif console.name == "PSP" %>
          <img src="https://upload.wikimedia.org/wikipedia/commons/4/46/Psp-1000.jpg" alt="Image de PSP">
        <% elsif console.name == "PS4" %>
          <img src="data:image/jpeg;base64," alt="Image de PS4">
        <% else %>
          <img src="https://source.unsplash.com/random" alt="">
        <% end %>
        <h4><%= console.name %></h4>
        <p><%= console.description %></p>
        <div class="button">
          <%= link_to "see this console", console_path(console) %>
        </div>
      </div>
    <% end %>
    </div>

    <div style="width: 100%; height: 600px;"
    data-controller="map"
    data-map-markers-value="<%= @markers.to_json %>"
    data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
    </div>

  <% end %>
</div>
